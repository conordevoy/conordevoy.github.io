	<script src="http://cdn.leafletjs.com/leaflet/v0.7.7/leaflet.js"></script>
	<script src="./basemapurl.js" type="text/javascript"></script>

	<script>
	var map = L.map('map');

	var zoomax = 19
	var mapaerial = new L.TileLayer(openstreetUrl, {
    	attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors'
		}, {maxZoom: zoomax, subdomains:'1234'});

	var baseLayers = {		
		"Map Quest Aerial": mapaerial,
	};

	map.addLayer(mapaerial);
	map.setView([53.5,-7.75],6); 	

	L.marker([53.35,-6.33]).addTo(map).bindPopup('I am here!<br>Contact me at:<br>conor.devoy@gmail.com');
	
	</script>
